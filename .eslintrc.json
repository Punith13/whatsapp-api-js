{
    "env": {
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": [
            "off"
        ]
    },
    "globals": {
        "fetch": "readonly"
    }
}
